{
  "name": "pdf-generator-service",
  "version": "2.0.0",
  "description": "Microserviço para geração de PDFs com Playwright - Arquitetura modular",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "npm run test:adapters",
    "test:adapters": "node --test tests/adapters",
    "test:smoke": "node tests/basic.test.js",
    "test:playwright": "playwright test",
    "lint": "eslint \"{src,public}/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "pdf",
    "generator",
    "playwright",
    "microservice",
    "nodejs"
  ],
  "author": "Geovane Silva",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "playwright": "^1.55.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2"
  }
}
